{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "name": "weatherschema",
    "description": "The JSON schema for the weather sim app",
    "type": "object",
    "required": [ "value", "timestamp", "seq" ],
    "properties": {
        "value": {
            "type": "number"
        },
        "timestamp": {
            "type": "string"
        },
        "seq": {
            "type": "number"
        }
    }
}